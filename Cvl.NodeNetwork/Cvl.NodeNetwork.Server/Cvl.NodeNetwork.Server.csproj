<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <OutputType>Library</OutputType>
    <Version>0.10.0</Version>
    <Description>Simple - C# Interface base communication module a'la WCF - server library .net 3.1</Description>
    <PackageProjectUrl>https://github.com/cv-lang/node-network</PackageProjectUrl>
    <RepositoryUrl>https://github.com/cv-lang/node-network</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageTags>wcf, communication</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <ApplicationIcon></ApplicationIcon>
    <Win32Resource />
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Cvl.NodeNetwork.Standard\Cvl.NodeNetwork.Standard.csproj" />
    <None Include="Icons\logo-nodenetwork-m.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Cvl.NodeNetwork" Version="0.9.1" />
  </ItemGroup>

</Project>
